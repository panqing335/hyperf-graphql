    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "authors": [
        {
            "name": "Li Yuzhao",
            "email": "562405704@qq.com",
            "homepage": "https://github.com/phper666/jwt-auth",
            "role": "Developer"
        }
    ],
    "description": "jwt-auth Component",
    "autoload": {
        "psr-4": {
            "Phper666\\JwtAuth\\": "src/"
        }
    },
    "autoload-dev": {
